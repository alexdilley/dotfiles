# Location of your dotfiles in $HOME
# DOTFILES=.dotfiles

# dotfiles to symlink
#
# srcfile:dstfile
# dstfile will be '.$srcfile' by default (with path removed)
#
[files]
dotsyncrc
bash/bashrc
bash/bash_profile
git/gitignore:.config/git/ignore
git/gitconfig
psql/psqlrc
rbenv
ssh
tmux.conf
vim
[endfiles]

## hostname                     type=masterhost, ANY or NONE    files=dotfile:tolink,another:tolink
[hosts]
george                          git=NONE
[endhosts]
